# --------- US national capital --------------------------------------

# The USA national capital Washington, DC (District of Columbia)
  38.883  -77.033 " Washington, DC"          color=gold   # -- USA national capital



# --------- US territories' capitals ---------------------------------

# American Samoa (Territory of American Samoa)
# American Samoa is an unorganized, unincorporated territory of the USA
 -14.267 -170.717 " Pago Pago, AS"           color=green  # -- American Samoa

# Guam (Territority of Guam)
# Guam is an organized, unincorporated territory of the USA
# (another name for the capital Hagatna is "Agana")
  13.467  144.750 " Hagatna, GU"             color=green  # -- Guam

# Northern Mariana Islands (Commonwealth of the Northern Mariana Islands
# Northern Mariana Islands is commonwealth in political union with the USA
  15.200  145.750 " Saipan, MP"              color=green  # -- Northern Mariana Islands

# Puerto Rico (Commonwealth of Puerto Rico)
# Puerto Rico is commonwealth associated with the USA
  18.483  -66.133 " San Juan, PR"            color=green  # -- Puerto Rico

# United States Virgin Islands
# United States Virgin Islands are an organized, unincorporated territory of the USA
  18.367  -64.933 " Charlotte Amalie, VI"    color=green  # -- United States Virgin Islands



# --------- US possessions without capitals --------------------------

# Baker Island
# Baker Island is an unincorporated territory of the US
# The island is uninhabited, it has a day beacon
   0.217 -176.517 " Baker Island, BI"        color=yellow # -- Baker Island

# Howland Island
# Howland Island is an unincorporated territory of the US
# The sland is uninhabited, it has a day beacon
   0.800 -176.633 " Howland Island"          color=yellow # -- Howland Island

# Jarvis Island
# Jarvis Island is an unincorporated territory of the US
# The sland is uninhabited (settlement abandoned) and has a day beacon
  -0.367 -160.050 " Jarvis Island"           color=yellow # -- Jarvis Island

# Johnston Atoll
# Johnston Atoll is an unincorporated territory of the US
# The atoll has an airstrip and a closing Navy site
  16.750 -169.517 " Johnston Atoll (JO)"     color=yellow # -- Johnston Atoll

# Kingman Reef
# Kingman Reef is an unincorporated territory of the US
# The reef is uninhabited
   6.400 -162.400 " Kingman Reef"            color=yellow # -- Kingman Reef

# Midway Islands
# Midway Islands are an unincorporated territory of the US
# The islands are inhabited by US Fish and Wildlife Service staff only and
# have a closed Navy base, 3 airports and a wildlife refuge open to the public
  28.217 -177.367 " Midway Islands (MW)"     color=yellow # -- Midway Islands

# Navassa Island
# Navassa Island is an unincorporated territory of the US
# The island is uninhabited
  18.400  -75.017 " Navassa Island"          color=yellow # -- Navassa Island

# Palmyra Atoll
# Palmyra Atoll an unincorporated territory of the US
# The atoll is inhabited by US Fish and Wildlife Service staff only; it has an airstrip
   5.867 -162.100 " Palmyra Atoll (PM)"      color=yellow # -- Palmyra Atoll

# Wake Island
# Wake Island is an unincorporated territory of the US
# The island has 1 airport and a military base (only civilian staff left)
  19.300  166.600 " Wake Island (WK)"        color=yellow # -- Wake Island



# --------- US associated states -------------------------------------

# Marshall Islands (Republic of the Marshall Islands)
# Marshall Islands are independent but governed in free association with the US
   7.083  171.133 " Majuro, MH"              color=white  # -- Marshall Islands

# Micronesia (Federated States of Micronesia)
# Micronesia is independent but governed in free association with the US
# (another name for the capital Palikir is "Kolonia")
   6.967  158.217 " Palikir, FM"             color=white  # -- Micronesia

# Palau (Republic of Palau)
# Palau is independent but governed in free association with the US
# current capital is Koror but a new capital is being built about 20 km northeast of Koror
   7.350  134.517 " Koror, PW"               color=white  # -- Palau


# --------- end ------------------------------------------------------

